<% if @clinks != nil then %>
	<% if !@clinks.empty? then %>
		<% @clinks.each do |infolink| %>
			<% @infopiece = infolink %>
			<table  class="infotable" name="cinfos<%= @page %>"  id="<%= @infopiece.id %>">
				<%= render "infopiece" %>
			</table>
		<% end %>
	<% else %>
		<h3>Empty</h3>
	<% end %>
<% else %>
	<h3>Null</h3>
<% end %>
